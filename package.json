{
  "name": "server-yelp-fusionai",
  "version": "0.1.5",
  "description": "MCP server for Yelp Fusion AI API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "test": "turbo run test",
    "docker": "npm run docker --workspace @waldzellai/clear-thought",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "smithery:yelp": "cd packages/yelp-fusionai-mcp && smithery deploy",
    "smithery:stochastic": "cd packages/server-stochasticthinking && smithery deploy",
    "smithery:clear-thought": "cd packages/server-clear-thought && smithery deploy",
    "deploy": "turbo run deploy",
    "publish-packages": "turbo run build lint test && changeset publish"
  },
  "keywords": [
    "yelp",
    "fusion",
    "ai",
    "mcp",
    "model-context-protocol"
  ],
  "bin": {
    "server-yelp-fusionai": "./dist/index.js"
  },
  "author": "glassBead for Waldzell AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/waldzellai/waldzell-mcp.git",
    "directory": "packages/yelp-fusionai-mcp"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/node": "^22.13.4",
    "prettier": "^3.1.0",
    "turbo": "^1.10.16",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.5.0",
    "chalk": "^5.4.1"
  },
  "packageManager": "yarn@4.6.0+sha512.5383cc12567a95f1d668fbe762dfe0075c595b4bfff433be478dbbe24e05251a8e8c3eb992a986667c1d53b6c3a9c85b8398c35a960587fbd9fa3a0915406728"
}
